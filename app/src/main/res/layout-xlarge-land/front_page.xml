<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frontpagelayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/lg">


    <Button
        android:layout_width="500dp"
        android:layout_height="100dp"
        android:textSize="40sp"
        android:text="@string/sq"
        android:id="@+id/bsq"
        android:background="@drawable/button_custom"
        android:textColor="#ffffff"
        android:layout_marginStart="87dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="104dp" />

    <Button
        android:layout_width="500dp"
        android:layout_height="100dp"
        android:textSize="40sp"
        android:text="@string/tq"
        android:id="@+id/btq"
        android:background="@drawable/button_custom"
        android:textColor="#ffffff"
        android:layout_marginEnd="84dp"
        android:layout_alignTop="@+id/bsq"
        android:layout_alignParentEnd="true" />

    <Button
        android:layout_width="500dp"
        android:layout_height="100dp"
        android:textSize="40sp"
        android:text="@string/see"
        android:id="@+id/bsee"
        android:background="@drawable/button_custom"
        android:textColor="#ffffff"
        android:layout_alignTop="@+id/brate"
        android:layout_alignStart="@+id/bsq" />

    <Button
        android:layout_width="500dp"
        android:layout_height="100dp"
        android:textSize="40sp"
        android:text="@string/ra"
        android:id="@+id/brate"
        android:background="@drawable/button_custom"
        android:textColor="#ffffff"
        android:layout_marginBottom="104dp"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/btq" />

</RelativeLayout>
